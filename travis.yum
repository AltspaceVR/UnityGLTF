os: osx
language: csharp
solution: "./GLTFSerialization/GLTFSerialization.sln"
install:
  - sudo python2 .scripts/install-unity.py --package Unity 2017.1.1f1
before_script:
  - sudo chmod -R a+rwx /Applications/Unity
script:
  - xbuild ./GLTFSerialization/GLTFSerialization.sln
  - .scripts/export-unity-package.sh